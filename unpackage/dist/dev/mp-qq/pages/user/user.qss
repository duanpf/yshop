.user {
  background-color: #f5f5f5;
  width: 100%;
  height: 100%;
}
.user .header {
    height: 200rpx;
    background-color: #73cbb6;
    border-radius: 0 0 30% 30%;
    padding: 40rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
}
.user .header .card {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
}
.user .header .card .image {
        width: 100rpx;
        height: 100rpx;
}
.user .header .card .desc {
        margin-left: 40rpx;
}
.user .header .card .desc .title {
          color: #FFFFFF;
          font-weight: 600;
}
.user .header .card .desc .user-id {
          color: #f5f5f5;
          font-size: 28rpx;
}
.user .header .set {
      margin-top: 20rpx;
}
.user .wrapper {
    position: relative;
    padding: 0 20rpx;
    margin-top: -60rpx;
    overflow: hidden;
}
.user .wrapper .nav {
      height: 140rpx;
      background-color: #FFFFFF;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-justify-content: space-around;
              justify-content: space-around;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      text-align: center;
      border-radius: 10rpx;
      font-size: 26rpx;
      padding: 0 20rpx;
      margin-bottom: 20rpx;
}
.user .wrapper .nav .item {
        width: 33.333333333%;
}
.user .wrapper .nav .item .title {
          color: #8c8c8c;
}
.user .wrapper .nav .item .num {
          font-size: 32rpx;
}
.user .wrapper .nav .item + .item {
        border-left: 1px solid #F1F1F1;
}
.user .wrapper .myOrder {
      background-color: #FFFFFF;
      border-radius: 10rpx;
      margin-bottom: 20rpx;
}
.user .wrapper .myOrder .myOrder-header {
        height: 100rpx;
        padding: 0 20rpx;
        -webkit-box-align: center;
        -webkit-align-items: center;
                align-items: center;
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-box-pack: justify;
        -webkit-justify-content: space-between;
                justify-content: space-between;
        border-bottom: 1px dashed #f5f5f5;
}
.user .wrapper .myOrder .myOrder-header .more {
          font-size: 28rpx;
          color: #777777;
}
.user .wrapper .myOrder .myOrder-desc {
        padding: 0 20rpx;
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        height: 160rpx;
        -webkit-justify-content: space-around;
                justify-content: space-around;
        font-size: 28rpx;
        -webkit-box-align: center;
        -webkit-align-items: center;
                align-items: center;
}
.user .wrapper .myOrder .myOrder-desc .item .icon {
          text-align: center;
}
.user .wrapper .myOrder .myOrder-desc .item .icon .image {
            width: 40rpx;
            height: 40rpx;
}
.user .wrapper .myService {
      background-color: #FFFFFF;
      border-radius: 10rpx;
      margin-bottom: 20rpx;
}
.user .wrapper .myService .myService-header {
        height: 100rpx;
        padding: 0 20rpx;
        -webkit-box-align: center;
        -webkit-align-items: center;
                align-items: center;
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-box-pack: justify;
        -webkit-justify-content: space-between;
                justify-content: space-between;
        border-bottom: 1px dashed #f5f5f5;
}
.user .wrapper .myService .myService-desc {
        padding: 20rpx;
}
.user .wrapper .myService .myService-desc .item {
          font-size: 26rpx;
          text-align: center;
}
.user .wrapper .myService .myService-desc .item .image {
            margin: 0 auto;
            margin-top: 20rpx;
            width: 80rpx;
            height: 80rpx;
}

